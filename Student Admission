#include <stdio.h>


int main()
{
    int age;
    float marks;
    char ID[20];
    char name[20];
    printf("Enter your ID");
    scanf("%s", ID);
    printf("Enter your name");
    scanf("%s",name);
    printf("Enter your age");
    scanf("%d",&age);
    printf("Enter your marks");
    scanf("%f",&marks);
 if (age>20&& marks>0 && marks<100)
{
    if(marks>65)
    {
        printf("You are Adimitted");
}        else
{
        printf("You are Not admitted");
    }
}
else
{
printf("INVALID DATA");
}

   
}
